package com.example.demo;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ApplicationContext;

import com.example.demo.bean.Student;
import com.example.demo.service.StudentService;

@SpringBootApplication
public class Day2SpringBoot1Application {

	public static void main(String[] args) {
	ApplicationContext ctx=SpringApplication.run(Day2SpringBoot1Application.class, args);
		
	StudentService ss =ctx.getBean(StudentService.class);
	
	ss.addStudent(new Student(104,"DDD",91));
	
	}

}
